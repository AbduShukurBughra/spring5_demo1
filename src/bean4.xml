<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="emp" class="com.abdushukur.spring5.bean.Emp">

      <property name="ename" value="Hakan"></property>
      <property name="gender" value="Man"></property>
      <property name="dept" ref="dept"></property>
      <property name="dept.dname" value="Technical"></property>
  </bean>
    <bean id="dept" class="com.abdushukur.spring5.bean.Dept">
        <property name="dname" value="IKA"></property>
    </bean>



</beans>